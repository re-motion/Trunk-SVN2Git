    xsi:schemaLocation="http://www.re-motion.org/Data/DomainObjects/Queries/1.0 ..\DomainObjects\Schemas\Queries.xsd">
	
	<query id="OrderSumQuery" type="scalar">
	    <storageProviderID>TestDomain</storageProviderID>        
	    <statement>select sum(quantity) from OrderView where Order_CustomerID = @customerID;</statement>
