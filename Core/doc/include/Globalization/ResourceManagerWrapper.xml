<ResourceManagerWrapper>
  <Constructor>
    <param name="resourceManager">
      The <c>ResourceManager</c> to be wrapped.
      Must not be <see langname="null"/>.
    </param>
    <param name="resourceManagers">
      The <c>ResourceManager</c> array to be wrapped.
      <c>resourceManagers</c> must not be emtpy and no instance may be <see langname="null"/>..
    </param>
  </Constructor>
  <GetAllStrings>
    <remarks>
      <para>
        If a duplicate ID is found, the ID from the last ResourceManager wins.
      </para><para>
        Fallback:
        <list type="number">
          <item>
            Resource hierarchy for current culture.
          </item>
          <item>
            Resource hierarchies for less specific cultures
          </item>
        </list>
      </para>
    </remarks>
  </GetAllStrings>
</ResourceManagerWrapper>