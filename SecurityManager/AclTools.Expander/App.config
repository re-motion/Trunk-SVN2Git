<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="remotion.data.domainObjects" type="Remotion.Data.DomainObjects.Configuration.DomainObjectsConfiguration, Remotion.Data.DomainObjects">
      <section name="storage" type="Remotion.Data.DomainObjects.Persistence.Configuration.PersistenceConfiguration, Remotion.Data.DomainObjects" />
      <section name="query" type="Remotion.Data.DomainObjects.Queries.Configuration.QueryConfiguration, Remotion.Data.DomainObjects"/>
    </sectionGroup>
  </configSections>

  <remotion.data.domainObjects xmlns="http://www.re-motion.org/Data/DomainObjects/Configuration/2.0">
    <storage defaultProviderDefinition="SecurityManager">
      <providerDefinitions>
        <add type="Remotion.Data.DomainObjects::Persistence.Rdbms.RdbmsProviderDefinition"
            name="SecurityManager"
            providerType="Remotion.SecurityManager::Persistence.SecurityManagerSqlProvider"
            connectionString="SecurityManager"/>
      </providerDefinitions>
    </storage>
    <query>
      <queryFiles>
        <add filename="SecurityManagerQueries.xml"/>
      </queryFiles>
    </query>
  </remotion.data.domainObjects>

  <connectionStrings>
    <!-- 
    <add name="SecurityManager" connectionString="Integrated Security=SSPI;Initial Catalog=RemotionSecurityManager;Data Source=localhost"/>
    -->
    <add name="SecurityManager" connectionString="Integrated Security=SSPI;Initial Catalog=RemotionSecurityManagerWebClientTest;Data Source=localhost" />
  </connectionStrings>
</configuration>